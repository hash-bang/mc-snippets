snippet breadcrumb
<ul class="breadcrumb">
	<li><a href="${1:#}"><i class="icon-home"></i></a> <span class="divider">/</span></li>
	<li><a href="${2:#}">${3:Foo}</a> <span class="divider">/</span></li>
	<li class="active">${4:Bar}</li>
</ul>
endsnippet

snippet btn
<a class="btn btn-default">${1:Button Text}</a>
endsnippet

snippet btndropdown
<div class="btn-group">
	<a class="btn btn-default dropdown-toggle" data-toggle="dropdown" href="#"> ${1:Button Text} <span class="caret"></span></a>
	<ul class="dropdown-menu">
		<li><a href="#">${2:Edit}</a></li>
		<li class="divider"></li>
		<li><a href="#">${3:Delete}</a></li>
	</ul>
</div>
endsnippet

