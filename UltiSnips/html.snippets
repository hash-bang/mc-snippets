snippet alert
<div class="alert alert-default">
	<button type="button" class="close" data-dismiss="alert">&times;</button>
	${1:Alert Message}

snippet alerterror
<div class="alert alert-error">
	${1:Alert Message}
</div>
endsnippet

snippet alertinfo
<div class="alert alert-info">
	${1:Alert Message}
</div>
endsnippet

snippet alertsuccess
<div class="alert alert-success">
	${1:Alert Message}
</div>
endsnippet

snippet badge
<span class="badge badge-default">${1:1}</span>
endsnippet

snippet badgedanger
<span class="badge badge-danger">${1:Text}</span>
endsnippet

snippet badgeinfo
