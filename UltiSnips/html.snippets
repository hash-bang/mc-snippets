snippet control
<div class="control-group">
	<label class="control-label">${1:Email}</label>
	<div class="controls">
		<input type="text">
	</div>
</div>
endsnippet

snippet dropdown
<div class="dropdown">
	<a href="#" class="btn dropdown-toggle" data-toggle="dropdown"><i class="icon-leaf"></i> <span class="caret"></span></a>
	<ul class="dropdown-menu">
		<li><a href="#"><i class="icon-edit"></i> ${1:Edit}</a></li>
		<li class="divider"></li>
		<li><a href="#"><i class="icon-trash"></i> ${2:Delete}</a></li>
	</ul>
</div>
endsnippet

snippet form
<form action="${1:?}" class="form-horizontal">
	<div class="control-group">
		<label class="control-label">Email</label>
		<div class="controls">
			<input type="text" placeholder="Email">
		</div>
	</div>
</form>
endsnippet

snippet form-group
<div class="form-group">
	<label class="col-sm-2 control-label">${1:Email}</label>
	<div class="col-sm-10">
		<input type="text" class="form-control" name="${2:email}"/>
	</div>
</div>
endsnippet

snippet field
<div class="control-group">
	<label class="control-label">${1:Label}</label>
	<div class="controls">
		${2:Input}
	</div>
</div>
endsnippet

snippet icon
<i class="icon-${1:leaf}"></i>
endsnippet

snippet i
<i class="icon-${1:leaf}"></i>
endsnippet

snippet inputappend
<div class="input-append">
	<input class="span2" type="text">
	<span class="add-on">${1:.00}</span>
</div>
endsnippet

snippet inputprepend
<div class="input-prepend">
	<span class="add-on">${1:@}</span>
	<input class="span2" id="prependedInput" type="text" placeholder="${2:Username}">
</div>
endsnippet

snippet label
<span class="label">${1:Text}</span>
